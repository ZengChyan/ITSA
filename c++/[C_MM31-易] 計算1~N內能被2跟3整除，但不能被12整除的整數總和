#include <iostream>
#include <stdio.h>
#include <cmath>
#include <algorithm>
#include <string>

using namespace std;

int main(){

    int num;

    while(cin >> num){
        int sum = 0;
        for(int i = 1; i <= num; i++){
            if(i % 2 == 0 && i % 3 == 0 && i % 12 != 0)
                sum += i;
        }
        cout << sum << endl;
    }

    return 0;
}
