#include <iostream>
#include <stdio.h>
#include <cmath>
#include <algorithm>

using namespace std;

int main(){

    double num[10];  
    double max;  
    double min;  
    for(int i = 0; i < 10; i++){  
        cin >> num[i];  
        if(i == 0){  
            max = num[i];  
            min = num[i];  
        }  
        else{  
            if(num[i] > max){  
                max = num[i];  
            }  
            if(num[i] < min){  
                min = num[i];  
            }  
        }  
    }  
    printf("maximum:%.2f\n", max);
    printf("minimum:%.2f\n", min);
    
    return 0;
}
