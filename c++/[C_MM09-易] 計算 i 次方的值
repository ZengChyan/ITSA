#include <iostream>
#include <stdio.h>
#include <cmath>
#include <algorithm>
#include <string>

using namespace std;

int main(){

    long long int num;
    while(cin >> num){  
        if(num > 31){
            cout << "Value of more than 31" << endl;
            continue;
        }
            
        printf("%lld\n", (long long int)pow(2, num));
    }  

    return 0;
}
