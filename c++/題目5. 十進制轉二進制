#include <iostream>
#include <stdio.h>
#include <cmath>

using namespace std;

int main(){

    int x;  
    cin >> x;
    if(x < 0){  
        x += 256;  
    }  
    int c[8];  
    for(int i = 7; i >= 0; i--){  
        c[i]= x/pow(2,i);  
        x %= (int)pow(2,i);  
        cout << c[i];
    }  
    cout << endl;

    return 0;
}
