#include <iostream>
#include <stdio.h>
#include <cmath>
#include <algorithm>
#include <string>

using namespace std;

int main(){

    int year;  
    while(cin >> year){  
        if(year % 4 == 0){  
            if(year % 100 != 0 || year % 400 == 0){  
                cout << "Bissextile Year" << endl;  
            }  
            else{  
                cout << "Common Year" << endl;  
            }  
        }  
        else{  
            cout << "Common Year" << endl;  
        }  
    }  

    return 0;
}
